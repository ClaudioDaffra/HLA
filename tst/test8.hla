
	u1 	: u16 ;
	u2 	: u16 ;
	u3	: u16 ;
	s1 	: s16 ;
	s2 	: s16 ;
	s3	: s16 ;
	
	fn main ( )  
	{
		u1 := 4567 ;
		u2 := 2u16 ;
		u3 := u1 >> u2 ;
		
		jmp esci;
		
		if (1)
		{
			asm {
				lda u3+0
				ldy u3+1
				jsr std.print_u16_dec
				lda #petscii.nl
				jsr kernel.chrout
			};

			asm {
				std_print_nl
			} ;
		} else {
			s1 := -4567 ;
			s2 := 2u16 ;
			s3 := s1 >> s2 ;
			
			asm {
				lda s3+0
				ldy s3+1
				jsr std.print_s16_dec
				lda #petscii.nl
				jsr kernel.chrout
			};
		} ;
		
	esci:;
	
		ret 0;
	} ;

	 